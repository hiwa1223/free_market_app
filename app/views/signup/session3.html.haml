            %i.fas.fa-chevron-down
        .sign-up-session3__body__contents__city.box
          .sign-up-session3__body__contents__city__title
            市区町村
            %span{class: "required"}必須
          .sign-up-session3__body__contents__city__form
            %input{class: "form city-form", placeholder: "例）大阪府浪速区"}
        .sign-up-session3__body__contents__number.box
          .sign-up-session3__body__contents__number__title
            番地
            %span{class: "required"}必須
          .sign-up-session3__body__contents__number__form
            %input{class: "form number-form", placeholder: "例）元町3-12-12"}
        .sign-up-session3__body__contents__building.box
          .sign-up-session3__body__contents__building__title
            建物
            %span{class: "optional"}任意
          .sign-up-session3__body__contents__building__form
            %input{class: "form building-form", placeholder: "例）なんばスカイオ"}
        .sign-up-session3__body__contents__tel.box
          .sign-up-session3__body__contents__tel__title
            電話
            %span{class: "optional"}任意
          .sign-up-session3__body__contents__tel__form
            %input{class: "form building-form", placeholder: "例）0120828828"}
        .sign-up-session3__body__contents__next
          =link_to session4_signup_index_path,class:"session3-next" do
            次へ進む
          =f.submit do
            送信
  .sign-up-session3__footer
  =render "users/users-footer"

